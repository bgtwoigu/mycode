<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.FileManager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zbin.FileManager.Category"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zbin.FileManager.Category_music" />
        <activity android:name="com.zbin.Util.Category_music_Util" />
        <activity android:name="com.zbin.Util.Category_video_Util" />
        <activity android:name="com.zbin.bean.Category_music_info" />
        <activity android:name="com.zbicom.zbin.bean.Category_video_info" />
        <activity android:name="com.zbicom.zbin.bean.storageinfo" />
        <activity android:name="com.zbin.FileManager.Category_video" />
        <activity android:name="com.zbin.FileManager.Category_phoneStorage" />
        <activity android:name="com.zbin.adapter.MyAdapter" />
        <activity android:name="com.zbin.FileManager.Category_images" />

        <service android:name="com.zbin.service.MusicService"
          >
        </service>
    </application>

</manifest>